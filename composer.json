{
    "name": "hulang/think-api-doc",
    "description": "ThinkPHP 8.0.0+ API 接口文档生成工具",
    "license": "Apache-2.0",
    "keywords": [
        "ThinkPHP",
        "ThinkPHP8",
        "apidoc",
        "api文档",
        "自动生成"
    ],
    "type": "think-extend",
    "authors": [
        {
            "name": "hulang",
            "email": "hulangfy@163.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "topthink/framework": "^8.0.0"
    },
    "autoload": {
        "psr-4": {
            "hulang\\apidoc\\": "src/"
        }
    },
    "extra": {
        "think": {
            "services": [
                "hulang\\apidoc\\Service"
            ],
            "config": {
                "apidoc": "src/config/apidoc.php"
            }
        }
    }
}
